(executables (names vector_tests matrix_tests examples)
  (libraries prbnmcn-proptest prbnmcn-linalg crowbar)
  (flags (:standard -open Basic_structures)))
(rule (alias runtest_vector_tests) (deps (:exe vector_tests.exe))
  (action (run %{exe})))
(rule (alias runtest_matrix_tests) (deps (:exe matrix_tests.exe))
  (action (run %{exe})))
(rule (alias runtest)
  (deps (alias runtest_vector_tests) (alias runtest_matrix_tests))
  (action (progn)))
